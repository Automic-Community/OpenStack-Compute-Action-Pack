<SCRIPT mode="1" state="1">
<MSCRI><![CDATA[: INC PCK.AUTOMIC_OPENSTACK.PUB.INCLUDE.ECHO_ON_OFF

! Read server list file content and publish first server id

: INC PCK.AUTOMIC_OPENSTACK.PUB.INCLUDE.READ_FILE UC4RB_OPS_FILE_PATH = UC4RB_OPS_LIST_SERVERS_XML

: SET &UC4RB_OPS_SERVER_ID# = ""
:  SET &ROOTHND# = XML_OPEN(STRING, &UC4RB_OPS_FILE_CONTENT#)
:  IF &ROOTHND# <> ""
:    SET &HND# = XML_GET_FIRST_CHILD(&ROOTHND#)
:    IF &HND# <> ""
:        SET &HND_NODE# = XML_GET_NODE_NAME(&HND#)
:        IF &HND_NODE# EQ "servers"
:          SET &CHILD_HND# = XML_GET_FIRST_CHILD(&HND#)
:          WHILE &CHILD_HND# <> ""
:            SET &NAME# = XML_GET_NODE_NAME(&CHILD_HND#)
:            IF &NAME# EQ "id"
:              SET &UC4RB_OPS_SERVER_ID# = XML_GET_NODE_TEXT(&CHILD_HND#)
:            ENDIF
:            SET  &CHILD_HND# = XML_GET_NEXTSIBLING(&CHILD_HND#)
:          ENDWHILE
:        ELSE
:          SET &UC4RB_OPS_SERVER_ID# = ""
:        ENDIF
:    ELSE
:        SET &UC4RB_OPS_SERVER_ID# = ""
:    ENDIF
:    PUBLISH &UC4RB_OPS_SERVER_ID# ,,"TOP"
:  ENDIF
:  XML_CLOSE]]></MSCRI>
</SCRIPT>
